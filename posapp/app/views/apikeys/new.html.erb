<% if @apikey.errors.any? %>
    <div id="error_messages">
      <h2>Errors:</h2>
      <ul>
        <% @apikey.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
        <% end%>
      </ul>
    </div>
<% end%>
<%=content_for :title do%>
    New application w/ api key
<% end %>
<h1>test!</h1>
<div class="col-md-6 col-md-offset-3">
  <%= form_for(:apikey) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
      <%= f.label :about %>
      <%= f.text_field :about, class: 'form-control' %>

      <%= f.submit "Add application", class: "btn btn-primary" %>
  <% end %>
</div>